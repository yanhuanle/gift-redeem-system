{
  "compilerOptions": {
    "target": "esnext",
    "jsx": "preserve",
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "experimentalDecorators": true,
    "baseUrl": ".",
    "module": "esnext",
    "moduleResolution": "Bundler",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "types": [
      "node",
      "unplugin-vue-router/client",
      "vite-plugin-pwa/client",
      "@intlify/unplugin-vue-i18n/messages"
    ],
    "allowJs": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "importHelpers": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "verbatimModuleSyntax": true,
    "skipLibCheck": true
  },
  "include": [
    "components.d.ts",
    "auto-imports.d.ts",
    "src/App.vue",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "src/typed-router.d.ts",
  ]
}